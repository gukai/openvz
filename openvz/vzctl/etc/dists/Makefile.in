install-distconfDATA: $(distconf_DATA)
	@$(NORMAL_INSTALL)
	test -z "$(distconfdir)" || $(MKDIR_P) "$(DESTDIR)$(distconfdir)"
	@list='$(distconf_DATA)'; test -n "$(distconfdir)" || list=; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(distconfdir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(distconfdir)" || exit $$?; \
	done

uninstall-distconfDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(distconf_DATA)'; test -n "$(distconfdir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	test -n "$$files" || exit 0; \
	echo " ( cd '$(DESTDIR)$(distconfdir)' && rm -f" $$files ")"; \
	cd "$(DESTDIR)$(distconfdir)" && rm -f $$files
